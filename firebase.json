{
  "functions": {
    "source": "functions"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "function": "sendMessage"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}